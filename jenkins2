pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building'
                
            }
        }
         stage('Deploye') {
            steps {
                echo 'Deploying'
                
            }
        }
         stage('Test') {
            steps {
                echo 'Testing'
                
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing'
                
            }
        }
    }
}
